{
  "name": "tpeg-samples",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run ./src/index.ts",
    "csv": "bun run ./src/peg/csv.ts",
    "json": "bun run ./src/peg/json.ts",
    "arith": "bun run ./src/arith/demo.ts",
    "arith:examples": "bun run ./src/arith/demo.ts --examples",
    "arith:repl": "bun run ./src/arith/repl.ts",
    "arith:test": "bun test ./src/arith/calculator.spec.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun build ./src --outdir ./dist",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck --declaration --outDir ./dist"
  },
  "dependencies": {
    "tpeg-combinator": "workspace:*",
    "tpeg-core": "workspace:*"
  }
}
